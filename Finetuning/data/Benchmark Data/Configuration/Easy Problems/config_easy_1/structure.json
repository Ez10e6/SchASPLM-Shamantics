{
  "instance_template": "cpu(\"Ryzen5\"). cpu(\"i5\"). gpu(\"GTX1660\"). gpu(\"RX580\"). mobo(\"B450\"). mobo(\"Z390\").",
  "generator": "1 { selected_cpu(C) : cpu(C) } 1. 1 { selected_gpu(G) : gpu(G) } 1. 1 { selected_mobo(M) : mobo(M) } 1.",
  "hard_constraints": [
    {
      "description": "Ryzen5 incompatible with non-B450 motherboards.",
      "asp_rule": ":- selected_cpu(\"Ryzen5\"), selected_mobo(M), M != \"B450\"."
    },
    {
      "description": "i5 incompatible with non-Z390 motherboards.",
      "asp_rule": ":- selected_cpu(\"i5\"), selected_mobo(M), M != \"Z390\"."
    },
    {
      "description": "RX580 incompatible with Z390 motherboard.",
      "asp_rule": ":- selected_gpu(\"RX580\"), selected_mobo(\"Z390\")."
    }
  ],
  "soft_constraints": [],
  "global_objective": ""
}